{
	"name": "blueprints",
	"displayName": "Blueprints",
	"description": "Workspace Blueprints",
	"icon": "assets/blueprints.png",
	"version": "0.0.1",
	"author": {
		"name": "Rudi Jansen van Vuuren"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:blueprints.blueprints"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "blueprints.blueprints",
				"title": "Blueprints"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "blueprints.blueprints",
					"group": "Blueprints"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@neotro/core": "^1.0.19",
		"@neotro/system": "^1.0.14"
	}
}
